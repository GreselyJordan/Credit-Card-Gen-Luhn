{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build"
    }
  ],
  "routes": [
    { "handle": "filesystem" },
    { "src": "/bin-lookup", "dest": "/bin-lookup.html" },
    { "src": "/sri-lookup", "dest": "/sri-lookup.html" },
    { "src": "/temp-mail", "dest": "/temp-mail.html" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
} 